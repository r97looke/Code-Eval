#!/usr/bin/env python
import sys
def long_line(line):
	longest = ''
	for x in line:
		if len(x)> len(longest):
			longest = x
	return longest
def main():
    with open(sys.argv[1]) as f:
        for line in f:
            line = line.strip().split()
            print long_line(line)
            
if __name__=="__main__":
    main()

