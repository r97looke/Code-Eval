#!/usr/bin/env python

import sys
import fileinput
import json

def myfileInput(file):
	for line in fileinput.input([file]):
		#print line
		json_encoded = json.dumps(line)
		print json_encoded
		encoded_data = json.loads(json_encoded)
		print encoded_data

		#now we have the json data as python data.
		#play with it


if __name__=="__main__":
	myfileInput(sys.argv[1])